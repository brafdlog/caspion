    "csv-stringify": "^5.6.2",
    "direct-vuex": "^0.12.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-google-oauth2": "^3.0.0",
    "electron-log": "^4.1.1",
    "electron-updater": "^5.2.4",
    "emittery": "^0.10.0",
    "googleapis": "^87.0.0",
    "israeli-bank-scrapers-core": "4.3.0",
    "keytar": "^7.4.0",
    "lint-staged": "~13.2.3",
    "lodash": "^4.17.15",
    "moment": "^2.29.4",
